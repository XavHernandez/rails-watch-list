<div class="container">
  <ul class="py-3">
    <% @lists.each do |list| %>
      <div class="py-3">
        <li>Nom: <%= link_to list.name, list_path(list) %></li>
        <li> <%= cl_image_tag list.photo.key,
      width: 200, height: 150, crop: :fill %> </li>
      </div>
    <% end %>
  </ul>
  <div>
    <%= link_to "Add a new List!", new_list_path %>
  </div>
</div>
